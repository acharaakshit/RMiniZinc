OBJECTS.tests = cpp_tests/test-runner.o cpp_tests/test-mzn_parse.o cpp_tests/test-set_params.o cpp_tests/test-mzn_eval.o cpp_tests/test-sol_parse.o  cpp_tests/test-getMissingPars.o 
