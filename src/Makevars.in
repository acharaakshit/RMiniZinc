CXX_STD = CXX11
PKG_CPPFLAGS = -I. @MZN_INCLUDE@
PKG_LIBS = @MZN_LIBS@  
OBJECTS.tests = cpp_tests/test-runner.o cpp_tests/test-sol_parse.o  
OBJECTS.sources = RcppExports.o mzn_parse.o mzn_eval.o sol_parse.o  
OBJECTS.helpers = helper_parse.o 
OBJECTS=$(OBJECTS.sources) $(OBJECTS.tests) $(OBJECTS.helpers) $(SOURCES:.cpp=.o)
strippedLib: clean $(SHLIB)
	if test -e "/usr/bin/strip"; then /usr/bin/strip --strip-debug $(SHLIB); fi
	$(R_HOME)/bin/Rscript -e "LIBMINIZINC_PATH <- '@MZN_LIBS@'; save(LIBMINIZINC_PATH, file='../data/config.RData')"
clean:
	rm -f $(OBJECTS) Makevars *.so
.phony: strippedLib clean
