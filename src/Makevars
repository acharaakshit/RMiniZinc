PKG_CXXFLAGS=-I. -I/usr/local/lib/libminizinc/include
PKG_CPPFLAGS=-I. -I/usr/local/lib/libminizinc/include
PKG_LDFLAGS=-lmzn
PKG_LIBS=-L/usr/local/lib/libminizinc	-lmzn 
OBJECTS.tests=cpp_tests/test-sol_parse.o cpp_tests/test-runner.o cpp_tests/test-mzn_parse.o cpp_tests/test-set_params.o cpp_tests/test-mzn_eval.o
OBJECTS.sources=RcppExports.o set_params.o mzn_parse.o mzn_eval.o sol_parse.o 
OBJECTS = $(OBJECTS.sources) $(OBJECTS.tests) $(SOURCES:.cpp=.o)
strippedLib: $(SHLIB)
	if test -e "/usr/bin/strip"; then /usr/bin/strip --strip-debug $(SHLIB); fi
.phony: strippedLib
all: clean
clean:
	rm -f $(OBJECTS.sources) $(OBJECTS.tests)